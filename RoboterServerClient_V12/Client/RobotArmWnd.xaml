<Window x:Class="Client.RobotArmWnd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="RobotArmWnd" Height="329" Width="347">
    <Grid>
        <RepeatButton Content="&lt;&lt;" Height="23" HorizontalAlignment="Left" Margin="6,12,0,0" 
                      Name="btn_left0"
                      VerticalAlignment="Top" Width="75"  />
        <Label Content="00" Height="28" HorizontalAlignment="Right" Margin="0,12,190,0" Name="lbl_actualPosition0" VerticalAlignment="Top" />
        <RepeatButton Content="&gt;&gt;" Height="23" HorizontalAlignment="Left" Margin="232,12,0,0" 
                      Name="btn_right0" 
                      VerticalAlignment="Top" Width="75" />
        <Label Content="Servo 0" Height="28" HorizontalAlignment="Right" Margin="0,11,130,0" Name="label1" VerticalAlignment="Top" />
        <RepeatButton Content="&lt;&lt;" Height="23" HorizontalAlignment="Left" Margin="6,39,0,0" Name="btn_left1" VerticalAlignment="Top" Width="75" />
        <Label Content="00" Height="28" HorizontalAlignment="Right" Margin="0,39,190,0" Name="lbl_actualPosition1" VerticalAlignment="Top" />
        <RepeatButton Content="&gt;&gt;" Height="23" HorizontalAlignment="Left" Margin="232,39,0,0" Name="btn_right1" VerticalAlignment="Top" Width="75" />
